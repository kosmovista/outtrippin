<div class="row" style="margin-top:100px">
  <div class="large-5 large-centered columns">
    <div class="panel">
      <h4><center>Login to your OutTrippin account</center></h4>
      <br />
      <%= form_for @user_session do |f| %>
      <%= f.text_field :email, placeholder: "Email" %>
      <%= errors_for(@user_session, :email) %>

      <%= f.password_field :password, placeholder: "Password" %>
      <%= errors_for(@user_session, :password) %>


      <%= f.submit "Go", class: "button radius expand" %>
      <% end %>
    </div>
    <%= link_to "Lost your password?", "#", style: "float:right;margin-bottom:30px" %>
  </div>
</div>